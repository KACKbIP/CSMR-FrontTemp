@model CSMR_Front.Models.AgentResponse

@if (@Model == null)
{
    <center><h2>@ViewBag.Message</h2></center>
}
else
{
<div class="divAll">
    <center><h2>Запишите и отправьте данные Агенту</h2></center>
    <br />
    <br />
    <div class="alert alert-secondary" style="margin-left: 5%">


        <div class="form-group d-flex row align-items-center justify-content-left">
            <label class="col-5" for="account">Аккаунт</label>
            <input type="text" class="form-control col-6" id="account" value="@Model.Login" required>
        </div>
        <div class="form-group d-flex row align-items-center justify-content-left">
            <label class="col-5" for="password">Пароль</label>
            <input type="text" class="form-control col-6" id="password" value="@Model.Password">
        </div>
        <div class="form-group d-flex row align-items-center justify-content-left">
            <label class="col-5" for="token">Токен</label>
            <input type="text" class="form-control col-6" id="token" value="@Model.Token" required>
        </div>
    </div>
</div>
} 